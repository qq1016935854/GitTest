<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	id="WebApp_ID" version="3.1">
	<display-name>Purejoy</display-name>

	<filter>
		<filter-name>f1</filter-name>
		<filter-class>com.itqf.web.filter.EncodingFilter</filter-class>
	</filter>


	<filter-mapping>
		<filter-name>f1</filter-name>
		<url-pattern>/*</url-pattern>
		<dispatcher>REQUEST</dispatcher>
		<dispatcher>FORWARD</dispatcher>
		<dispatcher>INCLUDE</dispatcher>
	</filter-mapping>


	<servlet>
		<servlet-name>BaseServlet</servlet-name>
		<servlet-class>com.itqf.web.servlet.BaseServlet</servlet-class>
	</servlet>
  <servlet>
    <servlet-name>UserServlet</servlet-name>
    <servlet-class>com.itqf.web.servlet.UserServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>FitnessfoodServlet</servlet-name>
    <servlet-class>com.itqf.web.servlet.FitnessfoodServlet</servlet-class>
  </servlet>






	<servlet-mapping>
		<servlet-name>BaseServlet</servlet-name>
		<url-pattern>/servlet/BaseServlet</url-pattern>
	</servlet-mapping>
  <servlet-mapping>
    <servlet-name>UserServlet</servlet-name>
    <url-pattern>/servlet/UserServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>FitnessfoodServlet</servlet-name>
    <url-pattern>/servlet/FitnessfoodServlet</url-pattern>
  </servlet-mapping>

	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>
</web-app>